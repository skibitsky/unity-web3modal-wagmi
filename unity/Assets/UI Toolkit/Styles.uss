.w3m-button {
    background-color: rgb(71, 161, 255);
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    border-radius: 20px;
    padding: 10px 18px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    margin-bottom: 12px;
}

.w3m-button:hover {
    background-color: rgb(89, 170, 255);
}
